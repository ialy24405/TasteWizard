<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="323.0" prefWidth="338.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.ResultCategory.ResultCategoryController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="58.0" prefWidth="338.0" text="Choose Category" textAlignment="CENTER" textFill="#4b7dbf">
         <font>
            <Font name="System Bold" size="27.0" />
         </font>
      </Label>
      <Region prefHeight="66.0" prefWidth="338.0" />
      <ComboBox fx:id="Category" prefHeight="44.0" prefWidth="242.0" promptText="Choose One" styleClass="ComboBox" />
      <Region layoutX="10.0" layoutY="128.0" prefHeight="39.0" prefWidth="338.0" />
      <Button fx:id="ProceedButton" mnemonicParsing="false" prefHeight="43.0" prefWidth="262.0" style="-fx-background-color: #4b7dbf;" text="Proceed">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</VBox>
