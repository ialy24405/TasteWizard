      <Button fx:id="ProceedButton" layoutX="322.0" layoutY="646.0" mnemonicParsing="false" prefHeight="44.0" prefWidth="324.0" style="-fx-background-color: #4b7dbf;" text="Back">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <TableView fx:id="ResultsTable" layoutX="11.0" layoutY="106.0" prefHeight="524.0" prefWidth="915.0">
